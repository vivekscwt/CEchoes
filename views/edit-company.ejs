$(document).ready(function() {
    function fetchStates(country) {
        $.ajax({
            url: '/getStates',
            method: 'GET',
            data: { country: country },
            success: function(response) {
                console.log('Response:', response); 

                var states = response.states || response; 

                if (Array.isArray(states)) {
                    var $stateDropdown = $('#main_address_state');
                    $stateDropdown.empty(); 
                    $stateDropdown.append('<option value="" selected disabled>Select State</option>'); 

                    states.forEach(function(state) {
                        $stateDropdown.append('<option value="' + state.id + '">' + state.name + '</option>');
                    });

                    var selectedStateId = '<%= company.main_address_state %>';
                    if (selectedStateId) {
                        $stateDropdown.val(selectedStateId);
                    }
                } else {
                    console.error('Unexpected response structure:', states);
                    $('#main_address_state').empty().append('<option value="" selected disabled>Select State</option>');
                }
            },
            error: function(xhr, status, error) {
                console.error('Error fetching states:', status, error);
                alert('Error fetching states. Please try again.');
                $('#main_address_state').empty().append('<option value="" selected disabled>Select State</option>');
            }
        });
    }

    var initialCountry = $('#main_address_country').val();
    if (initialCountry) {
        fetchStates(initialCountry);
    }

    $('#main_address_country').on('change', function() {
        var country = $(this).val();
        console.log("Selected country:", country);
        fetchStates(country);
    });
});


