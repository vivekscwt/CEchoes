                                                    class="form-control form-control-lg form-control-solid"
                                                    placeholder="Reply Content"
                                                    readonly><% if(complaint && complaint.company_reply_content){ %><%= complaint.company_reply_content %><% } %></textarea>
                                            </div>
                                            <!--end::Col-->
                                        </div>
                                        <% } %>
                                            <% if(complaint.reply_id) { %>
                                                <div class="row mb-6">
                                                    <!--begin::Label-->
                                                    <label class="col-lg-4 col-form-label fw-bold fs-6">
                                                        <span class="">User Reply</span>
                                                    </label>
                                                    <!--end::Label-->
                                                    <!--begin::Col-->
                                                    <div class="col-lg-8 fv-row">
                                                        <textarea name="reply_content" rows="8"
                                                            class="form-control form-control-lg form-control-solid"
                                                            placeholder="Reply Content" <%
                                                            if(currentUserData.user_type_id==3) {
                                                            %> readonly <% } %> ><% if(complaint && complaint.reply_content){ %><%= complaint.reply_content %><% } %></textarea>
                                                    </div>
                                                    <!--end::Col-->
                                                </div>
                                                <div class="row mb-6">
                                                    <input type="hidden" name="reply_id"
                                                        value="<%= complaint.reply_id %>" />
                                                    <input type="hidden" name="reply_previous_reason"
                                                        value="<%= complaint.reply_rejecting_reason %>" />
                                                    <!--begin::Label-->
                                                    <label class="col-lg-4 col-form-label  fw-bold fs-6">Want to reject
                                                        user reply</label>
                                                    <!--end::Label-->
                                                    <!--begin::Col-->
                                                    <div class="col-lg-8 fv-row">
                                                        <!--begin::Options-->
                                                        <div class="d-flex align-items-center mt-3">

                                                            <label
                                                                class="form-check form-check-inline form-check-solid">
                                                                <input class="form-check-input mt-1" id="rejectId"
                                                                    name="reply_status" type="checkbox" value="0" <%
                                                                    if(complaint.reply_status=='0' ){ %>checked <% } %>
                                                                    />
                                                                    <span
                                                                        class="fw-bold fs-6 badge badge-light-danger fw-bolder px-4 py-3">Yes</span>
                                                            </label>
                                                            <!--end::Option-->
                                                        </div>
                                                        <!--end::Options-->
                                                    </div>


                                                </div>
                                                <div id="reply_reject_area">

                                                </div>
                                                <div style="display: none; color: rgb(237, 16, 16);"
                                                    class="response-message col-lg-8 fv-row"></div>
                                                <% } %>
                                </div>

                                <% if(complaint.reply_id) { %>
                                    <div class="card-footer d-flex justify-content-end py-6 px-9">
                                        <input type="hidden" name="complaint_id" value="<%= complaint.id %>">
                                        <input type="hidden" name="company_id" value="<%= complaint.company_id %>">
                                        <button type="reset"
                                            class="btn btn-light btn-active-light-primary me-2">Discard</button>
                                        <button type="submit" class="btn btn-primary"
                                            id="kt_review_reply_update_form_submit">Submit</button>
                                    </div>
                                    <% } %>
                            </form>
                        </div>
                    </div>
                    <% } %>
                        <!--end::Basic info-->
            </div>
            <!--end::Container-->
        </div>
        <!--end::Post-->
    </div>
    <!--end::Content-->
