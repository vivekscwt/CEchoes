                                <input type="hidden" name="company_id" value="<%= complaint.company_id %>">
                                <button type="submit" class="btn btn-primary"
                                    id="kt_review_tag_update_form_submit">Submit</button>
                            </div>
                        </form>

                        <!--end::Form-->
                    </div>
                    <!--end::Content-->
                </div>

                <% if(complaint.company_reply_content) { %>
