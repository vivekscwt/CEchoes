                                            <div class="col-lg-8 fv-row">
                                                <input type="text" name="ticket_id" id="ticket_id"
                                                    class="form-control form-control-lg form-control-solid"
                                                    placeholder="Ticket Id"
                                                    value="<% if(complaint && complaint.ticket_id){ %><%= complaint.ticket_id %><% } %>"
                                                    <% if(currentUserData.user_type_id==3){ %>readonly <% } %> />
                                            </div>
                                            <!--end::Col-->
                                        </div>
                                        <div class="row mb-6">
                                            <!--begin::Label-->
                                            <label class="col-lg-4 col-form-label fw-bold fs-6">
                                                <span class="">Message</span>
                                            </label>
                                            <!--end::Label-->
                                            <!--begin::Col-->
                                            <div class="col-lg-8 fv-row">
                                                <input type="text" name="message" id="message"
                                                    class="form-control form-control-lg form-control-solid"
                                                    placeholder="Ticket Id"
                                                    value="<% if(complaint && complaint.message){ %><%= complaint.message %><% } %>"
                                                    <% if(currentUserData.user_type_id==3){ %>readonly <% } %> />
                                            </div>
                                            <!--end::Col-->
                                        </div>
                                        <div class="row mb-6">
                                            <!--begin::Label-->
                                            <label class="col-lg-4 col-form-label fw-bold fs-6">
                                                <span class="">Model Description</span>
                                            </label>
                                            <!--end::Label-->
                                            <!--begin::Col-->
                                            <div class="col-lg-8 fv-row">
                                                <input type="text" name="model_desc" id="model_desc"
                                                    class="form-control form-control-lg form-control-solid"
                                                    placeholder="Ticket Id"
                                                    value="<% if(complaint && complaint.model_desc){ %><%= complaint.model_desc %><% } %>"
                                                    <% if(currentUserData.user_type_id==3){ %>readonly <% } %> />
                                            </div>
                                            <!--end::Col-->
                                        </div>
                                        <div class="row mb-6">
                                            <!--begin::Label-->
                                            <label class="col-lg-4 col-form-label fw-bold fs-6">
                                                <span class="">Purchase Place</span>
                                            </label>
                                            <!--end::Label-->
                                            <!--begin::Col-->
                                            <div class="col-lg-8 fv-row">
                                                <input type="text" name="purchase_place" id="purchase_place"
                                                    class="form-control form-control-lg form-control-solid"
                                                    placeholder="Purchase Place"
                                                    value="<% if(complaint && complaint.purchase_place){ %><%= complaint.purchase_place %><% } %>"
                                                    <% if(currentUserData.user_type_id==3){ %>readonly <% } %> />
                                            </div>
                                            <!--end::Col-->
                                        </div>
                                        <div class="row mb-6">
                                            <!--begin::Label-->
                                            <label class="col-lg-4 col-form-label fw-bold fs-6">
                                                <span class="">Purchase Date</span>
                                            </label>
                                            <!--end::Label-->
