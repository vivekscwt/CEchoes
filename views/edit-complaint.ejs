                                            <div class="col-lg-8 fv-row">
                                                <input type="text" name="purchase_date" id="purchase_date"
                                                    class="form-control form-control-lg form-control-solid"
                                                    placeholder="Purchase Date"
                                                    value="<% if (complaint && complaint.purchase_date) { 
                                                        const date = new Date(complaint.purchase_date);
                                                        const formattedDate = date.toISOString().split('T')[0]; // Format to YYYY-MM-DD
                                                        %><%= formattedDate %><% 
                                                    } %>"
                                                    <% if (currentUserData.user_type_id == 3) { %>readonly <% } %>
                                                    onfocus="this.type='date'" onblur="this.type='text'" />
                                            </div>

                                            <!--end::Col-->
                                        </div>
                                        <div class="row mb-6">
                                            <!--begin::Label-->
                                            <label class="col-lg-4 col-form-label fw-bold fs-6">
                                                <span class="">Created Date</span>
                                            </label>
                                            <!--end::Label-->
                                            <!--begin::Col-->
                                            <div class="col-lg-8 fv-row">
                                                <input type="text" name="created_at" id="created_at"
                                                    class="form-control form-control-lg form-control-solid"
                                                    placeholder="Ticket Id"
                                                    value="<% if(complaint && complaint.created_at){ %><%= complaint.created_at %><% } %>"
                                                    <% if(currentUserData){ %>readonly <% } %> />
                                            </div>
                                            <!--end::Col-->
                                        </div>
                                        <div class="row mb-6">
                                            <!--begin::Label-->
                                            <label class="col-lg-4 col-form-label fw-bold fs-6">
                                                <span class="">Tags</span>
                                            </label>
                                            <!--end::Label-->
                                            <!--begin::Col-->
                                            <div class="col-lg-8 fv-row">
                                                <input class="form-control" name="tags" 
                                                    value="<% if (complaint && complaint.tags) { 
                                                        let tagsArray = JSON.parse(complaint.tags);
                                                        // Join the array of strings into a comma-separated string
                                                        let tagsString = tagsArray.join(', '); 
                                                        %><%= tagsString %><% 
                                                    } %>" 
                                                    id="kt_rating_tags" 
                                                    <% if (currentUserData.user_type_id == 3) { %>readonly <% } %> 
                                                />
                                            </div>

                                            <!--end::Col-->
                                        </div>

                                        <div class="row mb-6">
                                            <label class="col-lg-4 col-form-label fw-bold fs-6">
                                                <span class="">Category</span>
                                            </label>
                                            <div class="col-lg-8 fv-row">
                                                <select name="category_id" id="category_id"
