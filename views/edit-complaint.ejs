                                                        <option value="<%= category.id %>" <% if (complaint &&
                                                            complaint.category_id==category.id) { %> selected <% } %>>
                                                                <%= category.category_name %>
                                                        </option>
                                                        <% }) %>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="row mb-6">
                                            <label class="col-lg-4 col-form-label fw-bold fs-6">
                                                <span class="">Sub Category</span>
                                            </label>
                                            <div class="col-lg-8 fv-row" id="dynamic_sub_category_field_wrapper">
