        <script>
            var company_id = '<%= complaint.company_id %>';
            console.log("company_id", company_id);

            $(document).ready(function () {
                $('#category_id').change(function () {
                    var category_id = $(this).val();
                    console.log("Selected category_id:", category_id);

                    $('#sub_cat_id').empty().append($('<option>', {
                        value: 0,
                        text: 'Select Sub Category'
                    }));

                    if (category_id) {
                        $.ajax({
                            url: '/complain-sub-category',
                            method: 'GET',
                            data: { category_id: category_id },
                            success: function (data) {
                                if (data.status === 'ok' && data.data.length > 0) {
                                    $.each(data.data, function (i, item) {
                                        $('#sub_cat_id').append($('<option>', {
                                            value: item.id,
                                            text: item.category_name
                                        }));
                                    });

                                    var selectedSubCatId = '<%= complaint.sub_cat_id %>';
                                    if (selectedSubCatId) {
                                        $('#sub_cat_id').val(selectedSubCatId);
                                    }
                                }
                            },
                            error: function (error) {
                                Swal.fire({
                                    text: 'Error fetching subcategories',
                                    icon: 'error',
                                    buttonsStyling: false,
                                    confirmButtonText: 'Ok, got it!',
                                    customClass: {
                                        confirmButton: 'btn btn-primary'
                                    }
                                });
                            }
                        });
                    }
                });

                // Trigger category change event on page load if category is already selected
                if ($('#category_id').val()) {
                    console.log("Triggering category change");
                    $('#category_id').trigger('change');
                }
            });

        </script>