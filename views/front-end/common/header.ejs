  <style>
    .preloder {
      position: fixed;
      width: 100%;
      height: 100%;
      background: black;
      z-index: 999999999;
    }

    .preloder>img {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      margin: auto;
      width: 200px;
    }

    .loading {
      display: flex;
      flex-direction: row;
      position: absolute;
      top: 205px;
      bottom: 0;
      left: 0;
      right: 0;
      /* margin: auto; */
      margin: auto;
      width: 105px;
      height: 40px;
      text-align: center;
    }

    .loading__letter {
      font-size: 16px;
      font-weight: normal;
      letter-spacing: 2px;
      text-transform: uppercase;
      font-family: var(--nunito);
      color: #fff;
      animation-name: bounce;
      animation-duration: 2s;
      animation-iteration-count: infinite;
    }

    .loading__letter:nth-child(2) {
      animation-delay: .1s;
    }

    .loading__letter:nth-child(3) {
      animation-delay: .2s;
    }

    .loading__letter:nth-child(4) {
      animation-delay: .3s;
    }

    .loading__letter:nth-child(5) {
      animation-delay: .4s;
    }

    .loading__letter:nth-child(6) {
      animation-delay: .5s;
    }

    .loading__letter:nth-child(7) {
      animation-delay: .6s;
    }

    .loading__letter:nth-child(8) {
      animation-delay: .8s;
    }

    .loading__letter:nth-child(9) {
      animation-delay: 1s;
    }

    .loading__letter:nth-child(10) {
      animation-delay: 1.2s;
    }

    @keyframes bounce {
      0% {
        transform: translateY(0px)
      }

      40% {
        transform: translateY(-40px);
      }

      80%,
      100% {
        transform: translateY(0px);
      }
    }
  </style>
