              <script>
                function googleTranslateElementInit() {
                  new google.translate.TranslateElement({ pageLanguage: 'en' }, 'google_translate_element');
                  
                  // Timeout to ensure the iframe is fully loaded
                  setTimeout(setDefaultLanguage, 1000); // Adjust timing as needed
                }
                
                // Function to set the default selected language in the dropdown
                function setDefaultLanguage() {
                  // Get the iframe containing Google Translate
                  const googleTranslateFrame = document.querySelector('#google_translate_element iframe');
                  console.log("googleTranslateFrame",googleTranslateFrame);
                  
                  if (googleTranslateFrame) {
                    googleTranslateFrame.addEventListener('load', function() {
                      // Accessing the content of the iframe
                      const iframeDoc = googleTranslateFrame.contentDocument || googleTranslateFrame.contentWindow.document;
                      
                      // Finding the dropdown select element
                      const dropdown = iframeDoc.querySelector('.goog-te-combo');
                      
                      // Setting the value of the dropdown to 'hi' for Hindi if found
                      if (dropdown) {
                        dropdown.value = 'hi';
                        // Trigger change event manually
                        const event = new Event('change');
                        dropdown.dispatchEvent(event);
                      }
                    });
                  }
                }
                
                // Call googleTranslateElementInit when script is loaded
                document.addEventListener('DOMContentLoaded', function() {
                  googleTranslateElementInit();
                });
                
                </script>