                                            class="form-control form-control-lg form-control-solid"
                                            placeholder="Mata Title" value="<%= contacts.meta_title %>" />
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
                                <div class="row mb-6">
                                    <!--begin::Label-->
                                    <label class="col-lg-4 col-form-label fw-bold fs-6">
                                        <span class="">Meta Description</span>
                                    </label>
                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <input type="text" name="meta_desc" id="meta_desc"
                                            class="form-control form-control-lg form-control-solid"
                                            placeholder="Meta Description" value="<%= contacts.meta_desc %>" />
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
