                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <input type="email" name="email" id="email"
                                            class="form-control form-control-lg form-control-solid" placeholder="Email"
                                            value="<%= contacts.email %>" />
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <div class="card-header border-0 cursor-pointer" role="button"
                                    data-bs-target="#kt_account_profile_details" aria-expanded="true"
                                    aria-controls="kt_account_profile_details">
                                    <div class="card-title m-0">
                                        <h3 class="fw-bolder m-0">Socials Section</h3>
                                    </div>
                                </div>

                                <div class="row mb-6">
                                    <label class="col-lg-4 col-form-label fw-bold fs-6">
                                        <span>Facebook Link</span>
                                    </label>
                                    <div class="col-lg-8 fv-row">
                                        <input type="text" name="fb_link" id="fb_link"
                                            class="form-control form-control-lg form-control-solid"
                                            placeholder="Facebook Link" value="<%= socials.facabook %>" />
                                    </div>
                                </div>
                                <div class="row mb-6">
                                    <label class="col-lg-4 col-form-label fw-bold fs-6">
                                        <span>Twitter Link</span>
                                    </label>
                                    <div class="col-lg-8 fv-row">
                                        <input type="text" name="twitter_link" id="twitter_link"
                                            class="form-control form-control-lg form-control-solid"
                                            placeholder="Twitter Link" value="<%= socials.twitter %>" />
                                    </div>
                                </div>
                                <div class="row mb-6">
                                    <label class="col-lg-4 col-form-label fw-bold fs-6">
                                        <span>Linkedin Link</span>
                                    </label>
                                    <div class="col-lg-8 fv-row">
                                        <input type="text" name="linkedin_link" id="linkedin_link"
                                            class="form-control form-control-lg form-control-solid"
                                            placeholder="Linkedin Link" value="<%= socials.linkedin %>" />
                                    </div>
                                </div>
                                <div class="row mb-6">
                                    <label class="col-lg-4 col-form-label fw-bold fs-6">
                                        <span>Instagram Link</span>
                                    </label>
                                    <div class="col-lg-8 fv-row">
                                        <input type="text" name="instagram_link" id="instagram_link"
                                            class="form-control form-control-lg form-control-solid"
                                            placeholder="Instagram Link" value="<%= socials.instagram %>" />
                                    </div>
                                </div>
                                <div class="row mb-6">
                                    <label class="col-lg-4 col-form-label fw-bold fs-6">
                                        <span>Youtube Link</span>
                                    </label>
                                    <div class="col-lg-8 fv-row">
                                        <input type="text" name="youtube_link" id="youtube_link"
                                            class="form-control form-control-lg form-control-solid"
                                            placeholder="Youtube Link" value="<%= socials.youtube %>" />
                                    </div>
                                </div>



                            </div>
                            <!--end::Card body-->
                            <!--begin::Actions-->
                            <div class="card-footer d-flex justify-content-end py-6 px-9">
                                <button type="reset"
                                    class="btn btn-light btn-active-light-primary me-2">Discard</button>
                                <button type="submit" class="btn btn-primary"
                                    id="kt_account_profile_details_submit">Submit</button>
                            </div>
                            <!--end::Actions-->
                        </form>
                        <!--end::Form-->
                    </div>
                    <!--end::Content-->
                </div>
                <!--end::Basic info-->

                <!--end::Modals-->
            </div>
            <!--end::Container-->
        </div>
        <!--end::Post-->
    </div>
    <!--end::Content-->
    <%- include('../common/footer') -%>
